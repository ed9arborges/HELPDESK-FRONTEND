{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@assets/*": ["assets/*"],
      "@lib/*": ["lib/*"],
      "@features/*": ["features/*"],
      "@types/*": ["types/*"]
    },
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}
